{
  "name": "i18n-gen",
  "version": "0.2.7",
  "description": "Command line utility to generate i18n translation files from .tsv source to .yml and/or .json format",
  "scripts": {
    "test": "npm run compile && ./lib/index.js sample.tsv",
    "clean": "rimraf dist",
    "pretest": "npm run compile",
    "prepublish": "npm run compile",
    "compile": "lsc -c -b -m embedded --no-header -o lib/ src/index.ls"
  },
  "author": "Roger Lam <rlhk.open@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/rlhk/i18n-gen.git"
  },
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "i18n-gen": "lib/index.js"
  },
  "dependencies": {
    "prelude-ls": "^1.1.2"
  },
  "devDependencies": {
    "rimraf": "^2.4.3",
    "livescript":"^1.5.0"
  }
}
